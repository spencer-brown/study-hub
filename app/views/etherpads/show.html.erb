<div class="container">
	<div class="row">
		<h2><%= link_to 'Back', etherpads_path, class: "btn btn-default" %>
		<%= @etherpad.name %> Study Pad
	  <small><%= @etherpad.group.name %></small></h2>
	</div>
	<div class="row">
		<div id="examplePadBasic"></div>
	</div>
</div>

<%= link_to 'Edit', edit_etherpad_path(@etherpad), class: "btn btn-default" %>










<script type="text/javascript">
	var padname = "<%= @etherpad.name %>";
	$('#examplePadBasic').pad({'padId': padname}); // sets the pad id and puts the pad in the div
</script>